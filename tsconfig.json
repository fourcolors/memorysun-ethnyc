{
  "compilerOptions": {},
  "extends": "expo/tsconfig.base",
  "resolveJsonModule": true,
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "baseUrl": "./",
  "paths": {
    "@/*": ["./*"]
  },

  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "**/*.ttf",
    "**/*.otf"
  ]
}
